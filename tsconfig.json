{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
      "@constants/*": ["./constants/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts",
    "expo-env.d.ts",
    "types",
    ".expo/types/**/*.ts"
  ],
  "extends": "expo/tsconfig.base"
}
